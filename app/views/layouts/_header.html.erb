					<li><%= link_to "Doctor Log in", new_doctor_session_path %></li>
					<li><%= link_to "Admin Registration", new_clinic_admin_registration_path %></li>
					<li><%= link_to "Admin Log in", new_clinic_admin_session_path %></li>
				<% end %>
			</ul>
		</nav>
	</div>
</header>